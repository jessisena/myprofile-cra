  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "gulp": "^3.9.1",
    "icon-gen": "^2.0.0",
    "responsive-images-generator": "^0.2.2",
    "semantic-ui": "^2.4.2"
